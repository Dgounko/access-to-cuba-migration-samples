<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.northwind4cuba.web.inventorytransactiontype.InventoryTransactionTypeEdit"
        datasource="inventoryTransactionTypeDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.northwind4cuba.web.inventorytransactiontype">
    <dsContext>
        <datasource id="inventoryTransactionTypeDs"
                    class="com.company.northwind4cuba.entity.InventoryTransactionType"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="inventoryTransactionTypeDs">
            <column width="250px">
                <field property="typeName"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
